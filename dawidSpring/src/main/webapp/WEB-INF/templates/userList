      xmlns:th="http://www.thymeleaf.org">
<head>
    <title>Contacts</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
</head>
<body>


<div th:if="${not #lists.isEmpty(users)}">
	<div th:each="user: ${users}">
		<h3 th:text="${user.firstName + ' ' + user.secondName}"/>
		<h4 th:text="${user.table.title}" />
				
        <div th:include="column :: column(${user.table.backlog})"></div>
        <div th:include="column :: column(${user.table.nextTodo})"></div>
	</div>
</div>
	
	
<div th:insert="~{column :: copy}"></div>


</body>
</html>